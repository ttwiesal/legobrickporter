    "xml",
    "import"
  ],
  "author": "Alexander Wiesinger",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ttwiesal/legobrickporter/issues"
  },
  "homepage": "https://github.com/ttwiesal/legobrickporter#readme",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3"
  },
  "dependencies": {
    "axios": "^1.7.4",
    "form-data": "^4.0.0"
