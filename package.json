  "author": "Alexander Wiesinger",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ttwiesal/legobrickporter/issues"
  },
  "homepage": "https://github.com/ttwiesal/legobrickporter#readme",
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/jest": "^30.0.0",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "prettier": "^3.7.4"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "form-data": "^4.0.5"
  }
