{
  "name": "legobrickporter",
  "version": "1.0.0",
  "description": "converts lego order json to bricklink import xml.",
  "main": "index.js",
  "bin": {
    "legobrickporter": "./index.js"
  },
  "scripts": {
    "start": "node index.js --order=./samples/order.json --output=./output.xml --apiKey=1234",
    "test": "jest",
    "test:watch": "jest --watch",
